S -> GLOBAL FUNCION_PRINCIPAL GLOBAL EOF
GLOBAL -> ESTRUCTURA GLOBAL
GLOBAL -> FUNCION GLOBAL
GLOBAL -> VAR_GLOBAL GLOBAL
GLOBAL -> epsilon
FUNCION_PRINCIPAL -> funcion_principal INTRUCCIONES fin_principal
ESTRUCTURA -> estructura id ATRIBUTOS fin_estructura
ATRIBUTOS -> TIPO DEC_VARIABLE tk_pyc ATRIBUTOS
ATRIBUTOS -> epsilon
FUNCION -> funcion TIPO id tk_par_izq PARAMETROS tk_par_der hacer INTRUCCIONES fin_funcion
PARAMETROS -> TIPO id PARAMETROS2
PARAMETROS2 -> tk_coma TIPO id PARAMETROS2
PARAMETROS2 -> epsilon
PARAMETROS -> epsilon
VAR_GLOBAL -> TIPO DEC_VARIABLE tk_pyc
INSTRUCCIONES -> LEER INSTRUCCIONES
INSTRUCCIONES -> IMPRIMIR INSTRUCCIONES
INSTRUCCIONES -> SI INSTRUCCIONES
INSTRUCCIONES -> MIENTRAS INSTRUCCIONES
INSTRUCCIONES -> HACER_MIENTRAS INSTRUCCIONES
INSTRUCCIONES -> PARA INSTRUCCIONES
INSTRUCCIONES -> SELECCIONAR INSTRUCCIONES
INSTRUCCIONES -> DECLARACION_ASIGNACION INSTRUCCIONES
INSTRUCCIONES -> ROMPER INSTRUCCIONES
INSTRUCCIONES -> RETORNO INSTRUCCIONES
INSTRUCCIONES -> epsilon
TIPO -> id
TIPO -> PRIMITIVO
PRIMITIVO -> booleano
PRIMITIVO -> caracter
PRIMITIVO -> entero
PRIMITIVO -> real
PRIMITIVO -> cadena
LEER -> leer tk_par_izq IDENTIFICADOR tk_par_der tk_pyc
IMPRIMIR -> imprimir tk_par_izq IMPRESION tk_par_der tk_pyc
IMPRESION -> EXPRESION IMPRESION2
IMPRESION2 -> tk_coma EXPRESION IMPRESION2
IMPRESION2 -> epsilon
SI -> si tk_par_izq EXPRESION tk_par_der entonces INTRUCCIONES SI_NO fin_si
SI_NO -> si_no INSTRUCCIONES
SI_NO -> epsilon
MIENTRAS -> mientras tk_par_izq EXPRESION tk_par_der hacer INSTRUCCIONES fin_mientras
HACER_MIENTRAS -> hacer INSTRUCCIONES mientras tk_par_izq EXPRESION tk_par_der
PARA -> para tk_par_izq EXP_PARA tk_pyc EXPRESION tk_pyc EXPRESION tk_par_der hacer INSTRUCCIONES fin_para
EXP_PARA -> TIPO id tk_asig EXPRESION
SELECCIONAR -> seleccionar tk_par_izq EXPRESION tk_par_der entre CASOS fin_seleccionar
CASOS -> caso EXPRESION tk_dosp INSTRUCCIONES CASOS2
CASOS -> defecto tk_dosp INSTRUCCIONES
CASOS2 -> caso EXPRESION tk_dosp INSTRUCCIONES CASOS2
CASOS2 -> defecto tk_dosp INSTRUCCIONES
CASOS2 -> epsilon
DECLARACION_ASIGNACION -> PRIMITIVO DEC_VARIABLE tk_pyc
DECLARACION_ASIGNACION -> id DECLARACION_ASIGNACION2 tk_pyc
DECLARACION_ASIGNACION2 -> IDESTRUC ASIGNACION
DECLARACION_ASIGNACION2 -> ASIGNACION
DECLARACION_ASIGNACION2 -> CALL
DECLARACION_ASIGNACION2 -> DEC_VARIABLE
DEC_VARIABLE -> VARIABLE DEC_VARIABLE2
DEC_VARIABLE2 -> tk_coma VARIABLE DEC_VARIABLE2
DEC_VARIABLE2 -> epsilon
VARIABLE -> id ASIGNACION
ASIGNACION -> tk_asig EXPRESION
ASIGNACION -> epsilon
IDESTRUC -> tk_punto id IDESTRUC2
IDESTRUC2 -> tk_punto id IDESTRUC2
IDESTRUC2 -> epsilon
ROMPER -> romper tk_pyc
RETORNO -> retornar EXPRESION tk_pyc
CALL -> id tk_par_izq PARAMETROS_FUNCION tk_par_der
PARAMETROS_FUNCION -> EXPRESION PARAMETROS_FUNCION2
PARAMETROS_FUNCION -> epsilon
PARAMETROS_FUNCION2 -> tk_coma EXPRESION PARAMETROS_FUNCION2
PARAMETROS_FUNCION2 -> epsilon
IDENTIFICADOR -> id IDENTIFICADOR2 
IDENTIFICADOR2 -> tk_punto id IDENTIFICADOR2
IDENTIFICADOR2 -> epsilon
EXPRESION -> VALOR OPERADOR
OPERADOR -> tk_y VALOR 
OPERADOR -> tk_o VALOR 
OPERADOR -> tk_igual VALOR
OPERADOR -> tk_dif VALOR
OPERADOR -> tk_menor VALOR
OPERADOR -> tk_mayor VALOR
OPERADOR -> tk_menor_igual VALOR
OPERADOR -> tk_mayor_igual VALOR 
OPERADOR -> tk_mas VALOR
OPERADOR -> tk_menos VALOR 
OPERADOR -> tk_mult VALOR
OPERADOR -> tk_mod VALOR
OPERADOR -> tk_div VALOR
OPERADOR -> epsilon
VALOR -> tk_par_izq EXPRESION tk_par_der
VALOR -> IDENTIFICADOR
VALOR -> NUMERO_PALABRA
VALOR -> BOOLEANO
VALOR -> tk_menos VALOR
VALOR -> tk_mas VALOR
VALOR -> tk_neg VALOR
NUMERO_PALABRA -> tk_entero
NUMERO_PALABRA -> tk_real
NUMERO_PALABRA -> tk_caracter
NUMERO_PALABRA -> tk_cadena
BOOLEANO -> verdadero
BOOLEANO -> falso